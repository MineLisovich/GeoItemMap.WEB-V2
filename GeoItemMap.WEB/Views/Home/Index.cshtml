                var getByid_technoparck = geoitem.value;
            //    console.log(getByid_technoparck);
                $("#window-container").append(getInfoById_technoparck(getByid_technoparck));
                top_popup();
            }
            // Центр трансфера технологий
            else if (geoitem.value.geoType == 1) {
               
                var getByid_ctt = geoitem.value;
                $("#window-container").append(getInfoById_ctt(getByid_ctt));
                top_popup();
            }
            //ИНН проекты
            else if (geoitem.value.geoType == 2) {
                var getById_inn = geoitem.value;
                $("#window-container").append(getInfoByid_inn(getById_inn));
                top_popup();
            }
            //НТП проекты
            else if (geoitem.value.geoType == 3) {
                var getById_ntp = geoitem.value;
                 $("#window-container").append(getInfoById_ntp(getById_ntp));
                top_popup();
            }
        }
        else {
            const error = await response.json();
        }
    }
